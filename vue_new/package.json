    "core-js": "^3.6.5",
    "element-ui": "^2.13.1",
    "nprogress": "^0.2.0",
    "v-mec": "^1.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.3.0"
  }
