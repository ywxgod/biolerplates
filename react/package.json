{
  "name": "react_project_template",
  "version": "1.0.0",
  "description": "A react.js project template use webpack4 and antd",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "shx echo cleaning dirctory dist && shx rm -rf dist/*",
    "build:dev": "nodemon --watch build --exec \"webpack-dev-server --compress --config build/webpack.config.js --env development\"",
    "build:prod": "npm run clean && webpack --config build/webpack.config.js --env production",
    "build:test": "npm run clean && webpack --config build/webpack.config.js --env test",
    "serve:prod": "npm run build:prod && http-server -g -p 4203"
  },
  "author": "wyin",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "purgecss-webpack-plugin": "^1.4.0",
    "purify-css": "^1.2.5",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "antd": "^3.12.1",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
